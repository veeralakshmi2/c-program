
#include<stdio.h>
#include<string.h>
int main()
{
	char str1[10];
	int i,j,k=0;
	scanf("%s",str1);
	for(i=0;str1[i]!='\0';i++)
	{
		k++;
		
	}
	j=k-1;
	for(i=0,j;i<k/2;i++,j--)
	{
		if(str1[i]!=str1[j])
		{
			printf("not palondrome");
			return 0;
			
		}
		
	}
	printf("palindrome");
	return 0;
}
